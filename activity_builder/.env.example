# Strava Activity Data Analysis Configuration

# Path to the directory containing individual activity JSON files
# Default points to the activity_fetcher data directory
STRAVA_DATA_PATH=../activity_fetcher/data/individual_activities

# Optional: ChromaDB configuration (if you want to store analysis results)
CHROMA_HOST=chromadb
CHROMA_PORT=8000

# Optional: Output configuration
OUTPUT_DIR=./output
EXPORT_CSV=true
EXPORT_JSON=true
